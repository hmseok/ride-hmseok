# 로컬 개발 환경 설정
COMPOSE_PROJECT_NAME=ride-dev
SPRING_PROFILES_ACTIVE=dev

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_NAME=task_management
DB_USERNAME=root
DB_PASSWORD=password

# 백엔드 설정
BACKEND_PORT=8080
BACKEND_IMAGE=ride-backend:latest

# 프론트엔드 설정
FRONTEND_PORT=3000
FRONTEND_IMAGE=ride-frontend:latest
REACT_APP_API_URL=http://localhost:8080/api

# Nginx 설정 (개발 환경에서는 사용하지 않음)
NGINX_PORT=80
NGINX_SSL_PORT=443
