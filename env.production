# EC2 프로덕션 환경 설정
COMPOSE_PROJECT_NAME=ride-prod
SPRING_PROFILES_ACTIVE=prod

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_NAME=taskmanagement
DB_USERNAME=root
DB_PASSWORD=password

# 백엔드 설정
BACKEND_PORT=8080
BACKEND_IMAGE=ghcr.io/hmseok/ride-hmseok/backend:latest

# 프론트엔드 설정
FRONTEND_PORT=80
FRONTEND_IMAGE=ghcr.io/hmseok/ride-hmseok/frontend:latest
REACT_APP_API_URL=http://localhost:8080/api

# Nginx 설정
NGINX_PORT=80
NGINX_SSL_PORT=443
